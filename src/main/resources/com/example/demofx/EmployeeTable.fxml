<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demofx.EmployeeTableController">
   <children>
      <TableView fx:id="employeeTable" layoutX="10.0" layoutY="19.0" prefHeight="500.0" prefWidth="717.0">
        <columns>
          <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="First Name" />
          <TableColumn fx:id="lastNameColumn" prefWidth="93.0" text="Last Name " />
            <TableColumn fx:id="ssnColumn" prefWidth="82.0" text="SSN" />
            <TableColumn fx:id="birthdateColumn" prefWidth="81.0" text="Birthdate" />
            <TableColumn fx:id="addressColumn" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="sexColumn" prefWidth="75.0" text="Sex" />
            <TableColumn fx:id="salaryColumn" prefWidth="75.0" text="Salary" />
            <TableColumn fx:id="superSSnColumn" prefWidth="102.0" text="Super_ssn" />
            <TableColumn fx:id="dnumberColumn" prefWidth="58.0" text="Dnumber" />
        </columns>
      </TableView>
      <Button fx:id="seeDepartmentsButton" layoutX="594.0" layoutY="543.0" mnemonicParsing="false" onAction="#getDepartmentsInfo" text="See Departments" />
      <Button fx:id="createNewEmployeeButton" layoutX="14.0" layoutY="543.0" mnemonicParsing="false" onMouseClicked="#createNewEmployee" text="Create new employee" textAlignment="CENTER" textFill="#21a428" />
      <Button fx:id="deleteEmployeeButton" layoutX="177.0" layoutY="543.0" mnemonicParsing="false" onMouseClicked="#deleteEmployee" text="Delete employee" textAlignment="CENTER" textFill="#cd1717" />
      <Button fx:id="editButton" layoutX="330.0" layoutY="543.0" mnemonicParsing="false" onMouseClicked="#editEmployee" prefHeight="25.0" prefWidth="152.0" text="Edit existing employee" textAlignment="CENTER" textFill="#0f80c6" />
   </children>
</AnchorPane>
